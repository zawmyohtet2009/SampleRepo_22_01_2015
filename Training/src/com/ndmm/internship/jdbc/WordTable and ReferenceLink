//Create Table
CREATE TABLE `word` (
   `word_id` INT(10) NOT NULL AUTO_INCREMENT,
  `spelling` VARCHAR(100) NOT NULL,
  `part_of_speech` VARCHAR(100) NOT NULL,
  PRIMARY KEY (`word_id`)
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8;

//Insert Table
insert into word (spelling, part_of_speech) 
values ('tabemono', 'food-noun');

